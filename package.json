{
  "name": "apibrowser",
  "version": "2.0.0",
  "description": "DNN Connect Api Browser Module",
  "dnn": {
    "projectFolders": [
      "Server/ApiBrowser",
      "Themes/Skins/ApiBrowser",
      "Themes/Containers/ApiBrowser"
    ],
    "owner": {
      "name": "Peter Donker",
      "organization": "DNN Connect",
      "url": "http://dnn-connect.org",
      "email": "info@dnn-connect.org"
    },
    "pathsAndFiles": {
      "solutionFile": "./Connect.ApiBrowser.sln",
      "pathToAssemblies": "./bin",
      "devSiteUrl": "",
      "devSitePath": "",
      "excludeFilter": [
        "bin/",
        "obj/",
        "node_modules"
      ],
      "licenseFile": "./License.md",
      "releaseNotesFile": "",
      "releaseFiles": [
        "App_LocalResources/*.resx",
        "**/*.ascx",
        "**/*.cshtml",
        "**/*.vbhtml",
        "js/*.js",
        "css/*.css",
        "*.css",
        "**/*.html",
        "**/*.png",
        "**/*.jpg",
        "**/*.jpeg",
        "**/*.gif",
        "**/*.eot",
        "**/*.svg",
        "**/*.ttf",
        "**/*.woff",
        "**/*.woff2",
        "**/*.xml",
        "*.txt"
      ],
      "zipName": "Connect.ApiBrowser"
    }
  },
  "scripts": {
    "build": "set NODE_ENV=production&&webpack --config Client/webpack.config.js",
    "watch": "webpack --config Client/webpack.config.js --progress --watch --mode=development",
    "watch-server": "node watch.js"
  },
  "author": "Peter Donker",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jquery": "^3.5.6",
    "babel-loader": "^8.2.2",
    "babel-preset-solid": "^1.1.5",
    "bootstrap": "^5.1.1",
    "chokidar": "^3.5.2",
    "css-loader": "^6.3.0",
    "deepmerge": "^4.2.2",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^6.1.7",
    "fs-extra": "^10.0.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "postcss-loader": "^6.1.1",
    "rc-tree": "^5.2.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "marked": "^3.0.4",
    "solid-js": "^1.1.5"
  }
}
