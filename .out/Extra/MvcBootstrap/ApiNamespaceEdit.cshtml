
WebPage<ApiNamespace>
@using Connect.DNN.Modules.Conference.Common;
@using Connect.Conference.Core.Models.Conferences;
@using DotNetNuke.Web.Mvc.Helpers;

<div class="form-horizontal">
 <div class="form-group">
   <label for="NamespaceId" class="col-sm-2 control-label">@Dnn.LocalizeString("NamespaceId")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.NamespaceId, new { @class = "form-control", placeholder = Dnn.LocalizeString("NamespaceId"), required = &quot;true&quot;, type = &quot;number&quot; })
   </div>
 </div>
 <div class="form-group">
   <label for="ParentId" class="col-sm-2 control-label">@Dnn.LocalizeString("ParentId")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.ParentId, new { @class = "form-control", placeholder = Dnn.LocalizeString("ParentId"), required = &quot;true&quot;, type = &quot;number&quot; })
   </div>
 </div>
 <div class="form-group">
   <label for="ModuleId" class="col-sm-2 control-label">@Dnn.LocalizeString("ModuleId")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.ModuleId, new { @class = "form-control", placeholder = Dnn.LocalizeString("ModuleId"), required = &quot;true&quot;, type = &quot;number&quot; })
   </div>
 </div>
 <div class="form-group">
   <label for="NamespaceName" class="col-sm-2 control-label">@Dnn.LocalizeString("NamespaceName")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.NamespaceName, new { @class = "form-control", placeholder = Dnn.LocalizeString("NamespaceName"), required = &quot;true&quot; })
   </div>
 </div>
 <div class="form-group">
   <label for="LastQualifier" class="col-sm-2 control-label">@Dnn.LocalizeString("LastQualifier")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.LastQualifier, new { @class = "form-control", placeholder = Dnn.LocalizeString("LastQualifier"), required = &quot;true&quot; })
   </div>
 </div>
 <div class="form-group">
   <label for="Description" class="col-sm-2 control-label">@Dnn.LocalizeString("Description")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.Description, new { @class = "form-control", placeholder = Dnn.LocalizeString("Description") })
   </div>
 </div>
</div>
