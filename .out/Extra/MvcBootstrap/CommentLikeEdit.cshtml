
WebPage<CommentLike>
@using Connect.DNN.Modules.Conference.Common;
@using Connect.Conference.Core.Models.Conferences;
@using DotNetNuke.Web.Mvc.Helpers;

<div class="form-horizontal">
 <div class="form-group">
   <label for="CommentId" class="col-sm-2 control-label">@Dnn.LocalizeString("CommentId")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.CommentId, new { @class = "form-control", placeholder = Dnn.LocalizeString("CommentId"), required = &quot;true&quot;, type = &quot;number&quot; })
   </div>
 </div>
 <div class="form-group">
   <label for="UserId" class="col-sm-2 control-label">@Dnn.LocalizeString("UserId")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.UserId, new { @class = "form-control", placeholder = Dnn.LocalizeString("UserId"), required = &quot;true&quot;, type = &quot;number&quot; })
   </div>
 </div>
 <div class="form-group">
   <label for="Datime" class="col-sm-2 control-label">@Dnn.LocalizeString("Datime")</label>
   <div class="col-sm-10">
     @Html.TextBoxFor(m => m.Datime, new { @class = "form-control", placeholder = Dnn.LocalizeString("Datime"), required = &quot;true&quot;, type = &quot;datetime&quot; })
   </div>
 </div>
</div>
